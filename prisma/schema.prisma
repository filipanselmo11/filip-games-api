// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Game {
  id Int @id @default(autoincrement())
  name String
  gender String
  price String
  filename String
  createdAt DateTime? @default(now())

  @@map("games")
}

